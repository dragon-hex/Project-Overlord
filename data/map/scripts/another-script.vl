data:
    data    msg, ""
main:
    sysc    sysc_get_position
    inst    r0, r0
    inst    r1, r1

    move    "", msg
    move    "xpos: ", msg
    
    smgr    r0, msg
    smgr    " ypos: ", msg
    smgr    r1, msg
    smgr    ".", msg

    move    msg, r0
    call    print
    
    jump    main
